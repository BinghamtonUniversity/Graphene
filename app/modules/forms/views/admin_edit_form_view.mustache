<div class="panel-page">
<section class="panel panel-default">

	<div class="panel-heading">
		<h4 class="panel-title"><span class="fa fa-check-square-o"></span> {{name}}					
			<div class="btn-group pull-right">
				<a href="/form/{{id}}" class="btn btn-success" style="color:#fff"><i class="fa fa-eye"></i> Live View</a>
				<span class="btn btn-success options"><i class="fa fa-cogs"></i> Options</span> 
				<span class="btn btn-success save"><i class="fa fa-plus"></i> Save</span> 
			</div>
		</h4>
	</div>
				<div class="panel-body" style="padding:40px 0 0">
					<div id="optionscontainer">
							<div class="source view view_source" id="alt-sidebar" style="width:300px;float:right;position: relative;height: 600px;margin-right:15px;">
								<div><a class="btn btn-info fa fa-list reset-form-view hidden" style="position: absolute;right: 5px;z-index: 10;top: -10px;" href="javascript:cb.deactivate();"></a></div>
								<ul id="sortableList" class="list-group ">
									<li class="list-group-item list-group-item-success" data-type="uApp_textbox">Text</li>
									<li class="list-group-item list-group-item-success" data-type="uApp_select">Select</li>
									<li class="list-group-item list-group-item-success" data-type="uApp_checkbox">Checkbox</li>
								</ul>			
							<div id="form" class="form-cobler" style="overflow:hidden;height:100%"></div>				
							</div>
							<div id="editor" class="form-horizontal widget_container cobler_select well" style="min-height:500px;position:relative;margin-right:345px;margin-top:20px;margin-left:30px"></div>
					</div>
				</div>
</section>
</div>